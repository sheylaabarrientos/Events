    <action android:id="@+id/action_global_eventFragment" app:destination="@id/eventFragment" />
    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.events.MainActivity"
        android:label="MainActivity" />
    <fragment
        android:id="@+id/testeFragment"
        android:name="com.example.events.ui.fragments.TesteFragment"
